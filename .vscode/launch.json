  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Example1",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build Example1",
      "program": "${workspaceFolder}/Example1/bin/Debug/net7.0/Example1.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Example1",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
    }
  ]
}